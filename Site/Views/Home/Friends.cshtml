
@{
    ViewBag.Title = "Friends";
}

@model Site.Controllers.FacebookLists

@if (Model.DeletedMe!=null)
    {
<h2>Deleted Me:</h2>
<table id="friendsTable" class="table table-striped table-bordered">
    
        @foreach (var friend in Model.DeletedMe)
        {
            <tr>
                <td>@(friend.Name)</td>
            </tr>
        }
    

</table>
    }


@if (Model.OldList != null)
    {
    <h2>Friends</h2>
<table id="friendsTable" class="table table-striped table-bordered">

        @foreach(var friend in Model.OldList)
        { 
        <tr>
            <td>@(friend.Name)</td>
            <td><img src=@(friend.ImageURL) /></td>
        </tr>
        }
 

</table>
}
